#include<iostream>
using namespace std;

int main() {
    int num;
    int resultado;

    cout << "Ingresa un numero entre 1 y 100: ";
    cin >> num;

    resultado = num / 10;

    switch (resultado) {
        case 1:
        case 2:
        case 3:
        case 4:
        case 5:
             cout<<"El resultado es F"<<endl;
             break;
        case 6:
            cout << "El resultado es: D" << endl;
            break;
        case 7:
            cout << "El resultado es: C" << endl;
            break;
        case 8:
            cout << "El resultado es: B" << endl;
            break;
        case 9:
        case 10:
            cout << "El resultado es: A" << endl;
            break;
        default:
            cout << "El resultado no tiene una letra asignada." << endl;
    }

    return 0;
}

