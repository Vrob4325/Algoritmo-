#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de elementos: ";
    cin >> n;

    vector<int> arr(n);

   
    cout << "Ingrese los elementos:\n";
    for (int i = 0; i < n; ++i) {
        cin >> arr[i];
    }

    
    cout << "Elementos en orden inverso: ";
    for (int i = n - 1; i >= 0; --i) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}
